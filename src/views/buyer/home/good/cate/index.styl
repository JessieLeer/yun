.inner-main
	min-height: 100%
	padding-top: 60px
	background-color: #f5f5f5

.header
	background-color: #EAEAEA
	color: #333
	display: flex
	padding-top: 10px
	padding-bottom: 10px
	line-height: 40px
	position: fixed
	top: 0
	margin: 0 auto
	width: 100%
	max-width: 750px
	z-index: 9

.back
	width: 30px
	background: none
	
.search
	flex: 1
	margin-left: 10px
	margin-right: 10px
	color: #b3b3b3
	
.mint-cell
	min-height: 40px
	
.cate-wrapper
	display: flex
	
.cate-first
	width: 120px
	line-height: 40px
	background-color: white
	position: fixed
	top: 46px
	left: calc((100% - 750)/2)
	margin-right: 120px
	margin-block-start: 0em
	margin-block-end: 0em
	
.cate-con
	flex: 1
	padding-left: 120px
	
.cate-first-list
	padding-inline-start: 0
	
.cate-first-item
	list-style: none
	
.second-name
	background-color: #f5f5f5
	
.cate-thirds
	background-color: white
	padding-inline-start: 0
	padding-top: 10px
	padding-bottom: 10px
	margin-block-start: 0em
	margin-block-end: 0em
	
.cate-third
	list-style: none
	padding: 10px 10px 10px 10px
	margin-left: 5px
	margin-right: 5px
	width: calc((100% - 90px)/3)
	
.cate-image
	width: 100%
	max-width: 100px
	border-radius: 100%

.cate-name	
	margin-block-start: 0em
	margin-block-end: 0em
	
.cur
	background-color: #f5f5f5