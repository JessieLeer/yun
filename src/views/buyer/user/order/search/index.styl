.header
	background-color: #EAEAEA
	color: #333
	height: 40px
	line-height: 40px
	
.ml-10
	margin-left: 10px
	
.nav-btn
	background-color: #eaeaea
	margin: 0px 10px
	
.mint-search
	height: auto
	
.status
	color: #E43A3D
	
.order
	margin-top: 10px
	
.mt-10
	margin-top: 10px

.order-wrapper
	display: flex
	padding: 15px 10px
	background-color: white
	border-top: #f5f5f5 solid 1px
	
.order-image
	width: 100px
	margin-right: 10px
	
.order-info
	flex: 1
	
.order-title
	margin-block-start: 0em
	margin-block-end: 0em
	margin-bottom: 6px
	
.order-bread
	color: #B3B3B3
	margin-block-start: 0em
	margin-block-end: 0em
	
.price
	margin-top: 6px
	background-color: #f5f5f5
	height: 24px
	line-height: 24px
	
.price-now
	color: #E43A3D
	margin-left: 10px
	margin-right: 20px
	
.price-old
	color: #b3b3b3
	margin-right: 20px
	text-decoration: line-through
	
.cancel
	border: #d2d2d2 solid 1px
	background-color: white
	border-radius: 20px

.customer-name
	color: #333